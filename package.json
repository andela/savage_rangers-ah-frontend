{
  "name": "savage_rangers-ah-frontend",
  "version": "0.1.0",
  "scripts": {
    "start": "serve -s ./build",
    "dev": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "test:CI": "node_modules/.bin/jest --collectCoverage",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "test": "NODE_ENV=test && node_modules/.bin/jest --watchAll --collectCoverage",
    "eslint": "eslint . --cache --fix",
    "precommit": "lint-staged",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build --git git"
  },
  "repository": "https://github.com/andela/savage_rangers-ah-frontend",
  "license": "MIT",
  "private": false,
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "async": "^3.1.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "dotenv": "^8.0.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-loader": "^2.2.1",
    "express": "^4.17.1",
    "history": "^4.9.0",
    "image-webpack-loader": "^5.0.0",
    "jest": "^24.8.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-image-fallback": "^8.0.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "^16.8.6",
    "react-toastify": "^5.3.2",
    "redux": "^4.0.4",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "serve": "^11.1.0",
    "simple-react-validator": "^1.2.2",
    "socket.io-client": "^2.2.0",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "axios-mock-adapter": "^1.17.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.12.0",
    "redux-promise-middleware": "^6.1.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/App/ApiActions",
      "src/App/App",
      "configs/axios",
      "src/Redux/store",
      "src/Redux/Actions/index",
      "src/Redux/Reducers/index",
      "src/configs/urls"
    ],
    "setupFilesAfterEnv": [
      "./src/enzyme.js"
    ],
    "moduleNameMapper": {
      "\\.(css|jpg|png|scss)$": "fileMock.js"
    },
    "modulePaths": [
      "./src/__mocks__"
    ],
    "transformIgnorePatterns": [
      "./src/Style/scss/*",
      "./node_modules/react-toastify/dist/ReactToastify.css"
    ]
  }
}
